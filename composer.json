{
    "name": "armincms/taggable",
    "description": "A Laravel Nova tool.",
    "keywords": [
        "armincms"
    ],
    "license": "MIT",
    "require": { 
        "armincms/contract": "^1.0", 
        "superlatif/nova-tag-input": "^1.4"
    },
    "autoload": {
        "psr-4": {
            "Armincms\\Taggable\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [ 
                "Armincms\\Taggable\\ServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "version": "2.0.0"
}

